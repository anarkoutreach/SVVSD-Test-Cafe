  "dependencies": {
    "colors": "^1.4.0",
    "testcafe": "^1.16.0"
  },
  "devDependencies": {},
  "scripts": {
    "test-chrome-tee": "powershell \"npx testcafe chrome Tests/ -e | Tee -filepath ./tests.log\"",
